public class Solution {
    public int uniquePaths(int m, int n) {
        int[] dp = new int[n];
        // 下面这行等功能等同于dp[0] = 1 然后i从0开始
        // for(int i = 0; i < n; i++) dp[i] = 1;
        dp[0] = 1;
        for (int i = 1; i < m; i++) {
            for (int j = 1; j < n; j++) {
                dp[j] += dp[j - 1];
            }
        }
        return dp[n - 1];
    }
}
